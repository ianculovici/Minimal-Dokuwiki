# Minimal-Dokuwiki Configuration
# Copy this file to .env and modify as needed

# DokuWiki version to install/upgrade to (semantic versioning: YYYY.M.D.b)
VENDOR_VERSION=2024.2.6.b

# Number of backups to retain (default: 3)
BACKUP_RETENTION=3

# Timezone for the container (e.g., America/New_York, Europe/London)
TZ=UTC
